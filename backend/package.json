{
  "main": "src/server/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/server.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn src/server/server.ts",
    "knex": "knex --knexfile src/database/knexfile.ts",
    "migrate": "npm run knex migrate:latest",
    "migrate:rollback": "npm run knex migrate:rollback",
    "migrate:make": "npm run knex migrate:make",
    "seed": "npm run knex seed:run",
    "seed:make": "npm run knex seed:make",
    "test": "cross-env NODE_ENV=test ts-mocha --timeout 0 --require tests/setup.ts \"tests/**/*.{spec,test}.ts\"",
    "test:watch": "cross-env NODE_ENV=test mocha --require ts-node/register --require tests/setup.ts --extension ts --spec \"tests/**/*.{spec,test}.ts\" --timeout 10000 --recursive --exit --color --reporter spec --watch",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --require ts-node/register --require tests/setup.ts --extension ts --spec \"tests/**/*.{spec,test}.ts\" --timeout 10000 --recursive --exit --color --reporter spec"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.5.1",
    "@mailchimp/mailchimp_transactional": "^1.0.47",
