    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.11",
    "@types/jwt-simple": "^0.5.33",
    "@types/node": "^17.0.38",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^15.8.0",
    "jwt-simple": "^0.5.6",
    "knex": "^3.1.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.45",
    "typeorm-aurora-data-api-driver": "^2.4.2",
    "typeorm-typedi-extensions": "^0.4.1",
    "typescript": "^4.7.4",
